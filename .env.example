# Web Scout MCP Client Configuration

# Server Configuration
WEB_SCOUT_SERVER_PATH=python -m src.server
WEB_SCOUT_SERVER_HOST=localhost
WEB_SCOUT_SERVER_PORT=3000

# Client Configuration
WEB_SCOUT_CLIENT_TIMEOUT=30
WEB_SCOUT_CLIENT_MAX_RETRIES=3

# Output Configuration
WEB_SCOUT_OUTPUT_FORMAT=json
WEB_SCOUT_VERBOSE=false

# Cache Configuration
WEB_SCOUT_CACHE_ENABLED=true
WEB_SCOUT_CACHE_DIR=.webscout_cache
WEB_SCOUT_CACHE_TTL=3600